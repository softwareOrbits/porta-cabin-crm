# Environment Variables Template
# Copy this file to .env.local and fill in your values

# Application Environment
REACT_APP_ENV=development

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api

# Application Settings
REACT_APP_APP_NAME="Porta Cabin CRM"
REACT_APP_COMPANY_NAME="Porta Cabin Solutions"

# Feature Flags
REACT_APP_ENABLE_DIGITAL_SIGNATURE=true
REACT_APP_ENABLE_ZED_INTEGRATION=false

# External Services
REACT_APP_ZOHO_CLIENT_ID=your_zoho_client_id
REACT_APP_ZOHO_CLIENT_SECRET=your_zoho_client_secret

# Analytics (Optional)
REACT_APP_GA_TRACKING_ID=your_google_analytics_id

# Sentry for Error Tracking (Optional)
REACT_APP_SENTRY_DSN=your_sentry_dsn