{
  "name": "porta-cabin-crm",
  "build": {
    "command": "npm run build",
    "output": "build",
    "environment": {
      "NODE_VERSION": "18",
      "NPM_VERSION": "9"
    }
  },
  "routing": {
    "rules": [
      {
        "source": "/*",
        "destination": "/index.html",
        "status": 200
      }
    ]
  },
  "headers": [
    {
      "source": "/**/*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/static/**/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}